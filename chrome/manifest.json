{
  "manifest_version": 2,

  "name": "LaTeX GitHub Markdown",
  "description": "Show LaTeX formulas for GitHub Markdown files.",
  "version": "0.0.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
  "activeTab",
  "tabs",
  "cookies",
  "storage",
  "webNavigation",
  "https://github.com/",
  "https://gist.github.com/"
  ],
  "background": {
    "scripts": ["inject.js"]
  }
}
